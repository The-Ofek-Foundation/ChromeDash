{
	"manifest_version": 3,
	"name": "__MSG_appName__",
	"short_name": "__MSG_appName__",
	"version": "3.0.0",
	"description": "__MSG_appDescription__",
	"default_locale": "en",
	"action": {
		"default_icon": "browsericon.png",
		"default_popup": "browserpopup.html"
	},
	"permissions": [
		"storage"
	],
	"icons": {
		"16": "ChromeDash16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"64": "icon64.png",
		"128": "icon128.png"
	},
	"author": "Ofek Gila",
	"content_scripts": [
		{
			"all_frames": true,
			"match_about_blank": true,
			"matches": [
				"<all_urls>"
			],
			"js": [
				"general.js",
				"content.js"
			]
		}
	]
}